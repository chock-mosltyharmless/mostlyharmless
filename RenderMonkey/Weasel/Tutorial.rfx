     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
// Das sind comments

// void main(void) ist der Startpunkt, alles was zwischen den
// geschweiften Klammern ist, wird ausgefuehrt.
// Die Syntax erklaere ich spaeter.
void main(void)
{   
   // gl_FragColor is die Farbe, die der Pixel haben soll
   // gl_FragColor is vom Type "vec4", d.h. sie beinhaltet
   // 4 Werte: Rot, Gruen Blau und Alpha.
   // Die Werte reichen von 0.0 bis 1.0
   // Die Werte sind float, deswegen ist
   // glFragColor = vec4(1, 0, 0, 1) falsch
   // (auch wenn das auf NVidia Karten laueft)...
   gl_FragColor = vec4(1.0, 0.5, 0.0, 1.0);
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec2 pixel;

void main(void)
{
   gl_Position = vec4( gl_Vertex.xy, 0.0, 1.0 );
   gl_Position = sign( gl_Position );   
   
   // Texture coordinate for screen aligned (in correct range):
   pixel = (vec2( sign(gl_Vertex.x), sign(gl_Vertex.y) ) );
}]]></RmShaderCode></RmGLShader>
     <RmStreamMapReference NAME="streamMap" API=""/>
    </RmGLPass>
