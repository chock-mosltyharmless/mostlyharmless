     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec2 pixel;
uniform float fTime0_X;

void main(void)
{
   vec2 fractPixel;
   mat2 rotationMatrix; // 2x2 Matrix
   
   // Scale pixel in order to get zooming
   fractPixel = pixel * 5.0 * (sin(fTime0_X) + 1.1);
   
   // Generate matrix for rotation
   // You can write one operation into more than one line in order
   // to make it better readable.
   rotationMatrix = mat2(cos(fTime0_X), -sin(fTime0_X),
                         sin(fTime0_X), cos(fTime0_X));
   // rotate pixel by multiplying it with the rotation matrix
   fractPixel = rotationMatrix * fractPixel;
   
   // Get only the fractional part of pixel so that
   // the contents repeat
   fractPixel = fract(fractPixel);
   
   // "||" means OR
   if ((fractPixel.x < 0.5 && fractPixel.y < 0.5) ||
       (fractPixel.x > 0.5 && fractPixel.y > 0.5))
   {   
       gl_FragColor = vec4(1.0, 0.5, 0.0, 0.0);
   }
   else   
   {
       gl_FragColor = vec4(0.0, 0.0, 0.0, 0.0);
   }
}]]></RmShaderCode></RmGLShader>
