   <RmOpenGLEffect NAME="Bar" API="OpenGL" ACTIVE_CAMERA="">
    <RmStreamMap NAME="streamMap" API="">
     <RmStream NAME="" API="" STRIDE="20">
      <RmStreamChannel NAME="Position_0" API="" DATA_SIZE="12" DATA_TYPE="2" REGISTER="0" USAGE="0" USAGE_INDEX="0" ATTRIBUTE_NAME="rm_Vertex"/>
      <RmStreamChannel NAME="TexCoord_0" API="" DATA_SIZE="8" DATA_TYPE="1" REGISTER="0" USAGE="5" USAGE_INDEX="0" ATTRIBUTE_NAME="rm_TexCoord0"/>
     </RmStream>
    </RmStreamMap>
    <RmModelData NAME="Sphere" API="" FILE_NAME="..\Advanced\HeatHaze\ScreenAlignedQuad.3ds" ORIENTATION="0"/>
    <RmGLPass NAME="Effect" API="OpenGL" PASS_INDEX="0" ENABLED="TRUE">
     <RmModelReference NAME="Sphere" API=""/>
     <RmRenderStateBlock NAME="Render State" API="OpenGL"/>
     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
// Das ist eine Definiton, dass ich eine Variable "Pixel"
// vom Vertex Program bekomme, die den Typ vec2 hat
// (d.h. sie beinhaltet zwei Werte).
// Der Wert der vom Vertex Program in pixel geschrieben wird
// ist die aktuelle x- und y-Coordinate in einem Bereich von
// -1.0 bis +1.0
varying vec2 pixel;

// Das ist eine Definition, dass ich eine Variable "fTime0_X" vom
// Intro an sich bekomme.
// Die Variable ist vom Typ float (ein gleitpunkt Wert), und,
// wie man vielleicht erraten kann, steht darin die aktuelle Zeit.
// (Frag mich nicht, in welcher Einheit...)
uniform float fTime0_X;

// Kenne wir schon...
void main(void)
{   
   // Hier definiere ich, dass ich eine temporaere Variable mit Namen
   // barYPosition verwenden will. Sozusagen wird der Speicher
   // dafuer alloziert.
   float barYPosition;
   
   // Ich setze jetzt den Wert von barYPosition.
   // Ich will, dass die Bar sinus-foermig auf und ab geht.
   barYPosition = sin(fTime0_X);
   
   // Jetzt teste ich, ob der Pixel in der Bar ist:
   // dabei benutze ich pixel.y
   // pixel.y ist die zweite Komponente der globalen Variable pixel
   // && bedeutet "UND"
   if ((pixel.y > barYPosition - 0.1) && (pixel.y < barYPosition + 0.1))
   {
       // Wenn der Ausdruck hinter if wahr ist, dann wird alles
       // ausgefuehrt, was in den spitzen Klammern steht
       gl_FragColor = vec4(1.0, 0.5, 0.0, 1.0);
   }
   else // Optional kann man hier angeben was ansonsten passieren soll
   {
