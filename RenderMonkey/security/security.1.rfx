    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D RT;
uniform sampler2D noise16x16a;
uniform sampler2D noise16x16b;

varying vec2 vTexCoord;

varying vec3 rayOrigin;
varying vec3 rayDirection;

float noise(vec4 pos, int iterations, float reduction)
{
   float result = 0.;
   float intensity = 1.;

   for (int i = 0; i < iterations; ++i)
   {
      vec4 a = normalize(texture2D(noise16x16a, pos.rg) - 0.5);
      vec4 b = normalize(texture2D(noise16x16b, pos.ba) - 0.5);
      result += dot(a, b) * intensity;
      intensity *= reduction;
      pos *= 2.0;
   }
   
   return result;
}

void main(void)
{
   vec4 backbuffer = texture2D(RT, vTexCoord);

   // Calculate some distance based fog...
   float fogDist = backbuffer.a;
   vec4 fogColor = vec4(0.1, 0.2, 0.4, 1.0);
   
   // try raytracing for testing:
   vec3 center = vec3(0., 0., 0.);
   float dist = length(cross(rayDirection, rayOrigin - center)) /
                length(rayDirection);
   if (dist < 20.0)
   {
      fogColor = vec4(1.0, 0.4, 0.2, 1.0);
   }

   //gl_FragColor = (1.-backbuffer.a) * backbuffer;
   gl_FragColor = mix(backbuffer, fogColor, fogDist);
}]]></RmShaderCode></RmGLShader>
    <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[

varying vec2 vTexCoord;

varying vec3 rayOrigin;
varying vec3 rayDirection;

void main(void)
{
   // Clean up inaccuracies
   vec2 Pos = sign(gl_Vertex.xy);

   gl_Position = vec4(Pos.xy, 0, 1);
   // Image-space
   vTexCoord.x = 0.5 * (1.0 + Pos.x);
   vTexCoord.y = 0.5 * (1.0 + Pos.y);

   //calculate world space eye ray
   
   rayOrigin = (gl_ModelViewMatrixInverse * vec4(0,0,0,1)).xyz;
   vec4 edgePos = gl_ModelViewProjectionMatrixInverse*gl_Position;
   edgePos /= edgePos.w;
   //rayDirection = gl_Vertex.xyz - rayOrigin;
   rayDirection = edgePos.xyz - rayOrigin;

   //calculate homogenous coordinates for interpolation
   //gl_Position = gl_ModelViewProjectionMatrix * gl_Vertex;
}]]></RmShaderCode></RmGLShader>
