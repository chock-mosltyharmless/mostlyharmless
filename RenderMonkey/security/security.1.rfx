    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D noise16x16a;
uniform sampler2D noise16x16b;

uniform vec4 lightDir;
uniform float Ks;
uniform float Kd;
uniform float fTime0_X;

varying vec3 vNormal;
varying vec3 objectPosition;
varying vec4 vViewVec;

float noise(vec4 pos, int iterations, float reduction)
{
   float result = 0.;
   float intensity = 1.;

   for (int i = 0; i < iterations; ++i)
   {
      vec4 a = normalize(texture2D(noise16x16a, pos.rg) - 0.5);
      vec4 b = normalize(texture2D(noise16x16b, pos.ba) - 0.5);
      result += dot(a, b) * intensity;
      intensity *= reduction;
      pos *= 2.0;
   }
   
   return result;
}

void main(void)
{
   //vec3 base = texture2D(Base, vTexCoord).xyz;
   //vec3 base = texture2D(noise16x16a, vTexCoord*vec2(4.0)).xyz;
   float n = noise(vec4(objectPosition * 0.01, fTime0_X * 0.01), 7, 0.5);
   vec3 base = mix(vec3(1.0, 0.6, 0.3), vec3(0.4, 0.2, 0.1), vec3(abs(n)));

   // Basic lighting
   float diffuse = dot(lightDir.xyz, vNormal);
   float specular = pow(clamp(dot(reflect(normalize(objectPosition), vNormal), lightDir.xyz),0.0, 1.0), 16.0);
   vec3 light = Kd * diffuse * base + Ks * specular;

   // We'll use the distance to decide how much blur we want
   //float dist = length(vViewVec);
   float dist = vViewVec.z; // That is not very precise...

   gl_FragColor = vec4(light, dist);
}]]></RmShaderCode></RmGLShader>
    <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec3 vNormal;
varying vec3 objectPosition;
varying vec4 vViewVec;

uniform sampler2D noise16x16a;
uniform sampler2D noise16x16b;
uniform float fTime0_X;

float noise(vec4 pos, int iterations, float reduction)
{
   float result = 0.;
   float intensity = 1.;

   for (int i = 0; i < iterations; ++i)
   {
      vec4 a = normalize(texture2D(noise16x16a, pos.rg) - 0.5);
      vec4 b = normalize(texture2D(noise16x16b, pos.ba) - 0.5);
      result += dot(a, b) * intensity;
      intensity *= reduction;
      pos *= 2.0;
   }
   
   return result;
}

void main(void)
{
   float farClipPlane = 200.0;

   vec4 pos = gl_Vertex;
   pos.xyz += 2. * gl_Normal * noise(vec4(pos.xyz*0.01, 0.03*fTime0_X), 3, 0.5);

   gl_Position = gl_ModelViewProjectionMatrix * pos;
   
   // Eye-space lighting
   vNormal = gl_NormalMatrix * gl_Normal;
   
   // We multiply with distance scale in the vertex shader
   // instead of the fragment shader to improve performance.
   objectPosition = (gl_ModelViewMatrix * gl_Vertex).xyz;
   vViewVec = gl_Position;
   vViewVec.z /= farClipPlane;
}]]></RmShaderCode></RmGLShader>
